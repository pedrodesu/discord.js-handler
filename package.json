{
  "name": "discord.js-handler",
  "description": "Lightweight event and command handler for a discord.js project",
  "repository": {
    "type": "git",
    "url": "git://github.com/hSel3triK/discord.js-handler.git"
  },
  "keywords": [
    "discordapp",
    "discord",
    "api",
    "bot",
    "client",
    "handler",
    "events",
    "commands",
    "node"
  ],
  "version": "1.3.2",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start:publish": "concurrently \"npm run start:update\" \"npm run start:build\" \"npm publish\"",
    "start:update": "concurrently \"npm-check-updates\" \"npm install\"",
    "start:build": "concurrently \"rimraf lib\" \"tsc\""
  },
  "author": "el3triK_",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^13.13.1",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "npm-check-updates": "^4.1.2",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "discord.js": "^12.2.0"
  }
}
